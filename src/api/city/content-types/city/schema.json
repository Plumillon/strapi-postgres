{
  "kind": "collectionType",
  "collectionName": "cities",
  "info": {
    "singularName": "city",
    "pluralName": "cities",
    "displayName": "City"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "latitude": {
      "type": "float",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true
    },
    "activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity.activity"
    }
  }
}
